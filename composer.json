{
    "name": "laradvance/admin-config",
    "description": "Manage your database configuration as profiles",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "config"],
    "homepage": "https://github.com/laradvance/admin-config",
    "license": "MIT",
    "authors": [
        {
            "name": "fourn",
            "email": "fourn@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6",
        "laravel-admin-ext/config": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Fourn\\AdminConfig\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fourn\\AdminConfig\\AdminConfigServiceProvider"
            ]

        }
    }
}
